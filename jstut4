function MyStr(p1,p2){
    return p1+p2;
}

MyStr(1,2);


function toCelsius(fahrenheit) {
  return "freanhite is " + (5/9) * (fahrenheit-32);
}
toCelsius(97)
toCelsius (98)
function addition(a,b) {
  let x = a + b
  return x
}
console.log(addition(20,8))
function addition() {
  return  5 
}
console.log(addition())
